{
  "canvas": {
    "width": 128,
    "height": 128
  },
  "lineWidth": 20,
  "options": {
    "join": "miter",
    "miterLimit": 4,
    "cap": "square",
    "capResolution": 4
  },
  "pattern": "degenerate",
  "shader": {
    "type": "custom",
    "wgsl": "fn getColor(lineCoord: vec2f) -> vec4f {\n        return vec4f(0.0, 0.0, 0.0, 0.2);\n      }"
  },
  "blend": {
    "color": {
      "srcFactor": "src-alpha",
      "dstFactor": "one-minus-src-alpha",
      "operation": "add"
    },
    "alpha": {
      "srcFactor": "one",
      "dstFactor": "one",
      "operation": "add"
    }
  }
}
